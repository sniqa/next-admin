{
  "name": "next-admin",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "cd /data/next-admin/packages/client && bun run dev",
    "server": "cd /data/next-admin/packages/server &&  bun run --watch ./index.ts",
    "server-test": "cd packages/server bun run --watch ./test.ts",
    "build-client": "cd packages/client && bun run build",
    "build-server": "cd packages/server && bun build ./index.tsx --outdir ./build",
    "build-all": "bun run build-client & bun run build-server",
    "run-all": "bun run client & bun run server"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@next-admin/types": "0.1.0",
    "@next-admin/utils": "0.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "@types/ip": "^1.1.3",
    "prisma": "^5.8.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@mantine/core": "^7.4.2",
    "@mantine/dates": "^7.4.2",
    "@mantine/dropzone": "^7.4.2",
    "@mantine/form": "^7.4.1",
    "@mantine/hooks": "^7.4.2",
    "@mantine/modals": "^7.4.1",
    "@prisma/client": "5.8.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tabler/icons-react": "^2.46.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "exceljs": "^4.4.0",
    "ip": "^1.1.8",
    "jotai": "^2.6.1",
    "mantine-form-zod-resolver": "^1.1.0",
    "mantine-react-table": "alpha",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "socket.io": "^4.7.3",
    "socket.io-client": "^4.7.3",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  }
